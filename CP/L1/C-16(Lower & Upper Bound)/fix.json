    "body": [
      "int upperbound(vector<int> v, int q)",
      "{",
      "    int l = 0, r = v.size() - 1, ans = v.size() + 10;",
      "    while (l <= r)",
      "    {",
      "        int mid = (l + r) >> 1;",
      "        if (v[mid] <= q)",
      "        {",
      "            l = mid + 1;",
      "        }",
      "        else",
      "        {",
      "            ans = min(ans, mid);",
      "            r = mid - 1;",
      "        }",
      "    }",
      "    return ans;",
      "}"
    ],
    "description": ""
  }
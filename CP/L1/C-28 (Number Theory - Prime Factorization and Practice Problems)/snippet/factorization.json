    "body": [
      "vector<ll> PrimeFactors;",
      "",
      "void PrimeFactorization(ll n)",
      "{",
      "    PrimeFactors.clear();",
      "    // Call sieve function first for access primes till n",
      "    for (auto p : primes)",
      "    {",
      "        if (1LL * p * p > n)",
      "        {",
      "            break;",
      "        }",
      "        else if (n % p == 0)",
      "        {",
      "            while (n % p == 0)",
      "            {",
      "                PrimeFactors.push_back(p);",
      "                n /= p;",
      "            }",
      "        }",
      "    }",
      "    if (n > 1)",
      "    {",
      "        PrimeFactors.push_back(n);",
      "    }",
      "}"
    ],
    "description": ""

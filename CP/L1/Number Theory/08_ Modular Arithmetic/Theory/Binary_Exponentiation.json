"BinaryExponentiation": {
    "prefix": "BinaryExponentiation",
    "body": [
      "int BinaryExponentiation(int b, int p)",
      "{",
      "    int ans = 1;",
      "    while (p)",
      "    {",
      "        if (p & 1)",
      "        {",
      "            ans = (ans * 1LL * b) % mod;",
      "        }",
      "        b = (b * 1LL * b) % mod;",
      "        p >>= 1;",
      "    }",
      "    return ans;",
      "}"
    ],
    "description": "BinaryExponentiation"
  },